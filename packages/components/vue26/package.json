    "@storybook/vue-webpack5": "7.0.0-alpha.18",
    "@storybook/vue": "7.0.0-alpha.18",
    "@sterashima78/lit-practice-generator-cli": "*",
    "@babel/core": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "webpack": "5.74.0",
    "vue-loader": "15.9.8",
    "vue-template-compiler": "2.6.14",
