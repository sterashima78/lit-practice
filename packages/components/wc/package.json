{
  "name": "@sterashima78/lit-practice-wc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./my-accordion.js": {
      "types": "./lib/Accordion/my-accordion.d.ts",
      "default": "./lib/Accordion/my-accordion.js"
    },
    "./my-card.js": {
      "import": {
        "types": "./lib/Card/my-card.d.ts",
        "default": "./lib/Card/my-card.js"
      }
    },
    "./my-tab-group.js": {
      "import": {
        "types": "./lib/Tabs/my-tab-group.d.ts",
        "default": "./lib/Tabs/my-tab-group.js"
      }
    },
    "./my-tab.js": {
      "import": {
        "types": "./lib/Tabs/my-tab.d.ts",
        "default": "./lib/Tabs/my-tab.js"
      }
    },
    "./my-tab-panel.js": {
      "import": {
        "types": "./lib/Tabs/my-tab-panel.d.ts",
        "default": "./lib/Tabs/my-tab-panel.js"
      }
    }
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "custom-elements.json"
  ],
  "scripts": {
    "build:manufest": "cem analyze --globs 'src/**/*' --exclude 'src/**/*.stories.ts' --litelement",
    "build": "tsc",
    "postbuild": "npm-run-all build:manufest",
    "build:storybook": "storybook build",
    "serve:storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "lit": "2.2.8"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.6.3",
    "@storybook/web-components-webpack5": "7.0.0-alpha.18",
    "@storybook/web-components": "7.0.0-alpha.18",
    "@babel/core": "7.18.9",
    "babel-loader": "8.2.5",
    "@mdx-js/react": "1.6.22",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "lit-html": "2.2.7",
    "typescript": "*",
    "webpack": "5.74.0"
  }
}
