{
  "name": "@sterashima78/lit-practice-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/react-my-components.js",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/react-my-components.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "postbuild": "tsc --declaration --emitDeclarationOnly",
    "build": "vite build",
    "generate": "lit-practice-generator-cli -f ../wc/custom-elements.json -t react -o src/generated",
    "build:storybook": "storybook build",
    "serve:storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@lit-labs/react": "1.0.7",
    "@sterashima78/lit-practice-wc": "*"
  },
  "devDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@vitejs/plugin-react": "2.0.0",
    "typescript": "4.7.4",
    "vite": "3.0.4",
    "@storybook/react-webpack5": "7.0.0-alpha.18",
    "@storybook/react": "7.0.0-alpha.18",
    "@sterashima78/lit-practice-generator-cli": "*",
    "@sterashima78/lit-practice-token": "*",
    "webpack": "5.74.0",
    "@babel/core": "7.18.9",
    "jest-specific-snapshot": "4.0.0",
    "require-from-string": "2.0.2",
    "jest": "26.6.3",
    "@types/node": "18.6.2",
    "assert": "2.0.0",
    "eslint-plugin-react": "7.30.1",
    "eslint": "8.21.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  }
}
