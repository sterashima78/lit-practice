    "@sterashima78/lit-practice-generator-cli": "*",
    "@sterashima78/lit-practice-token": "*",
    "@storybook/vue3": "7.0.0-alpha.21",
    "@storybook/vue3-webpack5": "7.0.0-alpha.21",
    "@vitejs/plugin-vue": "3.0.1",
    "@vue/compiler-sfc": "3.2.37",
    "babel-loader": "8.2.5",
    "eslint": "8.21.0",
    "eslint-plugin-vue": "9.3.0",
    "typescript": "4.7.4",
    "vite": "3.0.3",
    "vue": "3.2.37",
    "vue-tsc": "0.39.2",
    "webpack": "5.74.0"
  }
